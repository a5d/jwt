{
  "swagger": "2.0",
  "info": {
    "title": "JWT"
  },
  "host": "localhost:4000",
  "basePath": "/api",
  "tags": [
    {
      "name": "API"
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/signup": {
      "post": {
        "tags": [
          "API"
        ],
        "summary": "Registration",
        "operationId": "apiSignup",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Email and password",
            "required": true,
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Registered"
          },
          "201": {
            "description": "Insert Error"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": [
          "API"
        ],
        "summary": "Login",
        "operationId": "apiLogin",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Email and password",
            "required": true,
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          }
        }
      }
    },
    "/logout": {
      "post": {
        "tags": [
          "API"
        ],
        "summary": "Logout",
        "operationId": "apiLogout",
        "responses": {
          "200": {
            "description": "successful operation"
          }
        }
      }
    },
    "/profile": {
      "get": {
        "tags": [
          "API"
        ],
        "summary": "Profile",
        "operationId": "apiProfile",
        "responses": {
          "200": {
            "description": "successful operation"
          }
        }
      }
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "required": [
        "email",
        "password"
      ],
      "properties": {
        "email": {
          "type": "string",
          "example": "Name"
        },
        "password": {
          "type": "string",
          "example": "12345"
        }
      }
    }
  }
}
